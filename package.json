    "@capacitor/status-bar": "7.0.0",
    "@ionic/vue": "^8.5.2",
    "@ionic/vue-router": "^8.0.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.8",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "ionicons": "^7.0.0",
    "mongodb": "^6.16.0",
    "swiper": "^11.2.4",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
